// 1. 1.1 Write a program to search a key element with in the given array of elements using linear search process.
#include <stdio.h>
void main() {
	int a[50], i, n, key,found =0;
	printf("Enter value of n : ");
	scanf("%d",&n);
	for (i=0;i<n;i++){
		printf("Enter element for a[%d] : ",i);
		scanf("%d",&a[i]);
	}
	printf("Enter key element : ");
	scanf("%d",&key);
	for(i=0;i<n;i++){
		if (a[i] == key) {
			found = 1;
			break;
		}
	}
	if (found) {
		printf("The key element %d is found at the position %d\n",key,i);
	}
		else {
			printf("The key element %d is not found in the array\n",key);
		}

}
